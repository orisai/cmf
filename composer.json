		"nette/schema": "^1.2.0",
		"nette/utils": "^3.2.0",
		"nextras/dbal": "~4.0.0",
		"nextras/migrations": "~3.1.4",
		"nextras/orm": "~4.0.0",
		"orisai/auth": "^1.0.0",
		"orisai/clock": "^1.0.0",
		"orisai/data-sources": "^1.0.0",
		"orisai/exceptions": "^1.0.0",
		"orisai/installer": "^1.0.0",
		"orisai/localization": "^1.0.0",
		"orisai/nette-application": "^1.0.0",
		"orisai/nette-auth": "^1.0.0",
		"orisai/nette-clock": "^1.0.0",
		"orisai/nette-console": "^1.0.0",
		"orisai/nette-di": "^1.0.0",
		"orisai/nette-http-client": "^1.0.0",
		"orisai/nette-monolog": "^1.0.0",
