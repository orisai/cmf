services:
	# Auth
	ori.front.auth.identityRefresher:
		factory: OriCMF\Front\Auth\FrontIdentityRefresher
		autowired: false
	ori.front.auth.firewall:
		factory: OriCMF\Front\Auth\FrontFirewall
		arguments:
			storage: @ori.ui.auth.loginStorage
			refresher: @ori.front.auth.identityRefresher

	# Error
	ori.front.error.presenter:
		factory: OriCMF\Front\Error\ErrorPresenter

	# Homepage
	ori.front.homepage.presenter:
		factory: OriCMF\Front\Homepage\HomepagePresenter

	# Login
	ori.front.login.presenter:
		factory: OriCMF\Front\Login\LoginPresenter
